{
  "name": "riyaroshan.github.io",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://riyaroshan.github.io",
  "scripts": {
    "dev": "nodemon server/server.js --ignore app/ --ignore test/ --exec babel-node",
    "build": "node -r @babel/register ./node_modules/webpack/bin/webpack --config ./webpack/webpack.production.config.js --progress --profile --colors && rimraf ./dist && mv ./tmp ./dist",
    "build-dev": "rimraf ./dist && babel-node ./node_modules/webpack/bin/webpack --config ./webpack/webpack.config.js --progress --profile --colors",
    "clean": "rimraf ./dist && rimraf ./tmp",
    "lint": "eslint app server test webpack",
    "test": "mocha --exit --require @babel/register ./test/test.js",
    "parse-routes": "node -r @babel/register parseRoutes.js",
    "predeploy": "npm run build && cp -r public/* dist/ && cp dist/index.html dist/404.html && npm run parse-routes && cp CNAME dist/",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@babel/core": "^7.22.20",
    "@babel/node": "^7.22.20",
    "@babel/polyfill": "^7.12.1",
    "@babel/register": "^7.22.20",
    "@babel/runtime": "^7.22.20",
    "@fortawesome/fontawesome-free": "^7.0.0",
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-brands-svg-icons": "^7.0.0",
    "@fortawesome/free-regular-svg-icons": "^7.0.0",
    "@fortawesome/react-fontawesome": "^0.2.3",
    "babel-loader": "^8.3.0",
    "compression": "^1.7.4",
    "core-js": "^3.32.2",
    "css-loader": "^3.6.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "file-loader": "^6.2.0",
    "gh-pages": "^6.1.1",
    "html-webpack-plugin": "^4.5.2",
    "jquery": "^3.7.1",
    "mini-css-extract-plugin": "^0.9.0",
    "morgan": "^1.10.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.8.1",
    "raw-loader": "^4.0.2",
    "react": "^16.14.0",
    "react-burger-menu": "^3.1.0",
    "react-dom": "^16.14.0",
    "react-ga": "^3.3.1",
    "react-helmet": "^6.1.0",
    "react-images": "^1.2.0-beta.7",
    "react-photo-gallery": "^8.0.0",
    "react-reveal": "^1.2.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scroll": "^1.9.3",
    "react-typist": "^2.0.5",
    "rimraf": "^5.0.1",
    "sass": "^1.69.0",
    "sass-loader": "^10.2.1",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "style-loader": "^1.3.0",
    "url-loader": "^4.1.1",
    "webpack": "^4.47.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.3",
    "webpack-hot-middleware": "^2.25.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-proposal-json-strings": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-async-to-generator": "^7.27.1",
    "@babel/plugin-transform-react-inline-elements": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chai": "^4.3.7",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.33.2",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1"
  }
}
